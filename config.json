{
    "DB": {
        "dbname": "${POSTGRES_DBNAME}",
        "user": "${POSTGRES_USER}",
        "password": "${POSTGRES_PASSWORD}",
        "host": "${POSTGRES_HOST}",
        "port": "${POSTGRES_PORT}"
    },
    "telegram": {
        "bot_token": "${TELEGRAM_BOT_TOKEN}",
        "chat_id": "${TELEGRAM_CHAT_ID}"
    },
    "filters": {
        "min_liquidity": 5000,
        "min_age_days": 3,
        "max_market_cap": 1000000,
        "volume_threshold": 100000
    },
    "blacklists": {
        "coin_blacklist": [],
        "dev_blacklist": []
    },
    "rugcheck": {
        "enabled": true,
        "url": "http://rugcheck.xyz/tokens/"
    },
    "fake_volume_detection": {
        "enabled": true,
        "threshold": 0.7
    },
    "trading": {
        "enabled": true,
        "risk_per_trade": 0.02,
        "stop_loss": 0.1,
        "take_profit": 0.3
    }
}